{
  "extends": [
    "airbnb",
    "prettier",
    "plugin:prettier/recommended",
    "eslint-config-prettier"
  ],
  "parser": "babel-eslint",
  "rules": {
    "import/no-unresolved": "off",
    "global-require": 0,
    "react/jsx-no-bind": 0,
    "prefer-destructuring": ["error", { "object": true, "array": false }],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "printWidth": 100
      }
    ]
  },
  "plugins": ["prettier"]
}
